{
  "name": "svea-bus",
  "private": false,
  "version": "1.0.2",
  "type": "module",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "main": "./dist/svea-bus.umd.cjs",
  "module": "./dist/svea-bus.js",
  "types": "./index.d.ts",
  "exports": {
    "types": "./index.d.ts",
    "import": "./dist/svea-bus.js",
    "require": "./dist/svea-bus.umd.cjs"
  },
  "scripts": {
    "dev": "vite",
    "declaration": "tsc lib/main.ts --declaration --emitDeclarationOnly --esModuleInterop --outFile index.d.ts",
    "build": "npm run declaration && tsc && vite build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:bench": "vitest bench"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.30.0",
    "mitt": "^3.0.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vitest": "^0.30.0"
  }
}
